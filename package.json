    "chai-string": "^1.3.0",
    "crypto-js": "^3.1.9-1",
    "electron-redux": "^1.3.1",
    "electron-store": "^2.0.0",
    "electron-window-state": "^4.0.1",
    "ethereum-client-binaries": "^1.6.4",
    "ethereum-keyfile-recognizer": "^1.0.2",
    "ethereumjs-abi": "^0.6.3",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-util": "^5.1.2",
    "fs-promise": "^2.0.0",
    "got": "^7.1.0",
    "i18next": "^8.4.3",
    "lodash": "^4.17.4",
    "log-rotate": "^0.2.7",
    "log4js": "^2.4.1",
    "lokijs": "^1.4.3",
    "minimongo-standalone": "^1.1.0-3",
    "node-fetch": "^1.7.3",
    "node-unzip-2": "^0.2.7",
    "numeral": "^2.0.6",
    "oboe": "^2.1.3",
