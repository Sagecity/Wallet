    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "prettier": "1.13.2",
    "pretty-quick": "^1.4.1",
    "redux-mock-store": "^1.3.0",
    "remotedev-server": "^0.2.4",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.1.0",
    "semver-compare": "^1.0.0",
    "shelljs": "^0.8.2",
