    "redux-persist-electron-storage": "^1.1.2",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.12",
    "semver": "^5.1.0",
    "solc": "0.4.24",
    "swarm-js": "^0.1.38",
    "typescript": "^2.2.2",
    "underscore": "^1.8.3",
    "underscore-deep-extend": "^1.1.5",
    "uuid": "^3.0.1",
    "web3": "^1.0.0-beta.33",
    "ws": "^5.1.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "co-mocha": "^1.2.0",
